public class Program_8 {
    public static void main(String[] args) {
        Test obj1 = new Test();
        Test obj2 = new Test();
        Test obj3 = new Test();
        System.out.println(Test.count);
    }
}

class Test {
    static int count;
    Test() {
        count++;
    }
}
// This program is used to calculate number of objects created of a class
