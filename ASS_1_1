public class ASS_1_1 {
    public static void main(String[] args) {
        Car myCar1 = new Car("Tata", "Nano");
        Car mycar2 = new Car(null, null);
        myCar1.get();
        mycar2.get();
        mycar2.set("Toyota", "Supra");
        mycar2.get();
    }
}

class Car {
    private String make;
    private String model;

    Car(String make, String model) {
        this.make = make;
        this.model = model;   
    }

    void set(String make, String model) {
        this.make = make;
        this.model = model;
    }

    void get() {
        System.out.println("Car maker : " + make);
        System.out.println("Model : " + model);
        System.out.println();
    }
}


